# Production Environment Variables for Render
NODE_ENV=production
PORT=10000

# Database will be provided by Render
# DB_HOST, DB_PORT, DB_NAME, DB_USER, DB_PASSWORD will be set by Render

# JWT Configuration
JWT_SECRET=your_super_secure_jwt_secret_here_change_this_in_production
JWT_EXPIRES_IN=24h

# CORS Configuration - Update with your Vercel URL
CORS_ORIGIN=https://quickzone-frontend.vercel.app

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
